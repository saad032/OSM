    <link href="~/Content/jquery.dataTables.css" rel="stylesheet" />
    <script src="~/Scripts/App/architecture.js"></script>
    <script src="~/Scripts/jquery-ui-1.10.4.js"></script>
    @*<script src="~/Scripts/jquery-1.10.2.js"></script>*@
    <script src="~/Scripts/Functions/scriptIST.js"></script>
    <script src="~/Scripts/responsive-tables.js"></script>

    @*<script src="~/Scripts/jquery.jqplugin.1.0.2.min.js"></script>*@
    <script src="~/Scripts/JqueryCalendar/jquery.plugin.js"></script>

    <script src="~/Scripts/JqueryCalendar/jquery.calendars.js"></script>
    <script src="~/Scripts/JqueryCalendar/jquery.calendars.plus.js"></script>  
    @*<script src="~/Scripts/JqueryCalendar/jquery.calendars.all.js"></script>*@
