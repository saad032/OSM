    <script src="~/Scripts/JqueryCalendar/jquery.calendars.picker.js"></script>
    <script src="~/Scripts/JqueryCalendar/jquery.calendars.islamic.js"></script>

    <link rel="shortcut icon" href="img/favicon/favicon.png">
