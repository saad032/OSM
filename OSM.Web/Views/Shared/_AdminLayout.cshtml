</head>
<body>
    <div>
        <header>
            <div class="navbar navbar-fixed-top bs-docs-nav" role="banner">

                <div class="container">
                    <!-- Menu button for smallar screens -->
                    <div class="navbar-header">
                        <button class="navbar-toggle btn-navbar" type="button" data-toggle="collapse" data-target=".bs-navbar-collapse"><span>Menu</span></button>
                        <a href="#" class="pull-left menubutton hidden-xs"><i class="fa fa-bars"></i></a>
                        <!-- Site name for smallar screens -->
                        <a href="index" class="navbar-brand">Community Welfare, Embassy of Pakistan Riyadh@*<span class="bold">System</span>*@</a>
                    </div>

                    <!-- Navigation starts -->
                    <nav class="collapse navbar-collapse bs-navbar-collapse" role="navigation">

                        <!-- Links -->
                        @if (@User.Identity.GetUserName() != null && @User.Identity.GetUserName() != "")
{
                        <ul class="nav navbar-nav pull-right">
                            <li class="dropdown pull-right user-data">
                                <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                                    @*<img src='@("~/" + Session[" ProfileImage"])'>*@ Hello <span class="bold">@Session["FullName"]</span> <b class="caret"></b>
                                </a>

                                <!-- Dropdown menu -->
                                <ul class="dropdown-menu">
                                    <li><a href="~/Account/Profile"><i class="fa fa-user"></i> Profile</a></li>
                                    <li><a href="~/Account/Manage"><i class="fa fa-cogs"></i> Change Password</a></li>
                                    <li><a href="~/Account/LogOff"><i class="fa fa-key"></i> Logout</a></li>
                                </ul>
                            </li>



                        </ul>
}
                    </nav>

                </div>
            </div>
        </header>

        <!-- Main content starts -->

        <div class="content">

            <!-- Sidebar -->
            <div class="sidebar">
                <div class="sidebar-dropdown sideparpad"><a href="#">Navigation</a></div>

                <!--- Sidebar navigation -->
                <!-- If the main navigation has sub navigation, then add the class "has_sub" to "li" of main navigation. -->
                @Html.Action("LoadMenu", "Menu")

            </div>
            <!-- Sidebar ends -->
            <div class="">
                @RenderBody()
                <div class="clearfix"></div>
            </div>
            <!-- Mainbar ends -->

        </div>
        <!-- Content ends -->
        <!-- Footer starts -->
        <footer>
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <!-- Copyright info -->
                        <p class="copy">Copyright &copy; 2014 | <a href="#">Community Welfare, Embassy of Pakistan Riyadh</a> </p>
                    </div>
                </div>
            </div>
        </footer>
        <input type="hidden" id="siteURL" value="@(ConfigurationManager.AppSettings["SiteURL"].ToString())" />
        @if (ViewBag.MessageVM != null)
        {
            <input type="hidden" id="Message" value="@ViewBag.MessageVM.Message" />
            <input type="hidden" id="IsSaved" value="@ViewBag.MessageVM.IsSaved" />
            <input type="hidden" id="IsUpdated" value="@ViewBag.MessageVM.IsUpdated" />
            <input type="hidden" id="IsError" value="@ViewBag.MessageVM.IsError" />
        }
        <input type="hidden" id="siteURL" value="@(ConfigurationManager.AppSettings["SiteURL"].ToString())" />
    </div>
    <!-- Footer ends -->
    <!-- JS -->
    @*<script src="js/jquery.js"></script> <!-- jQuery -->*@
    <script src="~/Scripts/App/Admin/js/bootstrap.js"></script> <!-- Bootstrap -->
    <script src="~/Scripts/App/Admin/js/jquery-ui-1.9.2.custom.min.js"></script> <!-- jQuery UI -->
    <script src="~/Scripts/App/Admin/js/fullcalendar.min.js"></script> <!-- Full Google Calendar - Calendar -->
    <script src="~/Scripts/App/Admin/js/jquery.rateit.min.js"></script> <!-- RateIt - Star rating -->
    <script src="~/Scripts/App/Admin/js/jquery.prettyPhoto.js"></script> <!-- prettyPhoto -->
    <script src="~/Scripts/toastr.js"></script>
    <!-- Morris JS -->
    @*<script src="~/Scripts/App/Admin/js/raphael-min.js"></script>
        <script src="~/Scripts/App/Admin/js/morris.min.js"></script>*@

    <!-- jQuery Flot -->
    @*<script src="~/Scripts/App/Admin/js/excanvas.min.js"></script>
        <script src="~/Scripts/App/Admin/js/jquery.flot.js"></script>
        <script src="~/Scripts/App/Admin/js/jquery.flot.resize.js"></script>
        <script src="~/Scripts/App/Admin/js/jquery.flot.pie.js"></script>
        <script src="~/Scripts/App/Admin/js/jquery.flot.stack.js"></script>*@

    <!-- jQuery Notification - Noty -->
    <script src="~/Scripts/App/Admin/js/jquery.noty.js"></script> <!-- jQuery Notify -->
    <script src="~/Scripts/App/Admin/js/themes/default.js"></script> <!-- jQuery Notify -->
    <script src="~/Scripts/App/Admin/js/layouts/bottom.js"></script> <!-- jQuery Notify -->
    <script src="~/Scripts/App/Admin/js/layouts/topRight.js"></script> <!-- jQuery Notify -->
    <script src="~/Scripts/App/Admin/js/layouts/top.js"></script> <!-- jQuery Notify -->
    <!-- jQuery Notification ends -->
    <!-- Daterangepicker -->
    <script src="~/Scripts/App/Admin/js/moment.min.js"></script>
    <script src="~/Scripts/App/Admin/js/daterangepicker.js"></script>

    <script src="~/Scripts/App/Admin/js/sparklines.js"></script> <!-- Sparklines -->
    <script src="~/Scripts/App/Admin/js/jquery.gritter.min.js"></script> <!-- jQuery Gritter -->
    <script src="~/Scripts/App/Admin/js/jquery.cleditor.min.js"></script> <!-- CLEditor -->
    <script src="~/Scripts/App/Admin/js/bootstrap-datetimepicker.min.js"></script> <!-- Date picker -->
    @*<script src="~/Scripts/App/Admin/js/jquery.uniform.min.html"></script> <!-- jQuery Uniform -->*@
    <script src="~/Scripts/App/Admin/js/jquery.slimscroll.min.js"></script> <!-- jQuery SlimScroll -->
    <script src="~/Scripts/App/Admin/js/bootstrap-switch.min.js"></script> <!-- Bootstrap Toggle -->
    <script src="~/Scripts/App/Admin/js/filter.js"></script> <!-- Filter for support page -->
    <script src="~/Scripts/App/Admin/js/custom.js"></script> <!-- Custom codes -->
    <script src="~/Scripts/App/Admin/js/charts.js"></script> <!-- Charts & Graphs -->

    <script src="~/Scripts/jquery.dataTables.js"></script>
    @*<script src="~/Scripts/App/Admin/js/datatables/js/jquery.dataTables.js"></script>*@

    <script src="~/Scripts/App/Admin/js/index.js"></script> <!-- Index Javascripts -->
</body>
</html>
